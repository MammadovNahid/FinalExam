@model List<Setting>
@{
    int count = 0;
}
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Employee Table</h4>
                <a asp-controller="team" asp-action="create" class="btn btn-success">Create</a>
                <div class="table-responsive pt-3">
                    <table class="table table-dark">
                        <thead>
                            <tr>
                                <th>
                                    #
                                </th>
                                <th>
                                    Key
                                </th>
                                <th>
                                    Value
                                </th>
                                <th>
                                    Settings
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                count++;
                                <tr>
                                    <td>
                                        @count
                                    </td>
                                    <td>
                                        @item.Key
                                    </td>
                                    <td>
                                        @item.Value
                                    </td>
                                    <td>
                                        <a asp-controller="service" asp-action="update" asp-route-id="@item.Id" class="btn btn-warning">Update</a>

                                    </td>

                                </tr>

                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
